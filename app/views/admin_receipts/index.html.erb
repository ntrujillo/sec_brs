<p id="notice"><%= notice %></p>

<h1>Admin Receipts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_receipts.each do |admin_receipt| %>
      <tr>
        <td><%= link_to 'Show', admin_receipt %></td>
        <td><%= link_to 'Edit', edit_admin_receipt_path(admin_receipt) %></td>
        <td><%= link_to 'Destroy', admin_receipt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Receipt', new_admin_receipt_path %>
